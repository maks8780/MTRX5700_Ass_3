<launch>

  <param name="use_sim_time" value="true"/>
  <param name="base_frame" value="base_footprint"/>

  <node pkg="gmapping_q2" type="odom_to_path.py" name="odom_to_path">
  </node>

  <node pkg="gmapping_q2" type="transform_listener.py" name="transform_listener">
  </node>
  
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
    
    <rosparam>
      odom_frame: odom
      map_update_interval: 5.0
      maxUrange: 20
      sigma: 0.05
      kernelSize: 1
      lstep: 0.05
      astep: 0.05
      iterations: 5
      lsigma: 0.075
      ogain: 3.0
      lskip: 0
      srr: 0.1
      srt: 0.2
      str: 0.1
      stt: 0.2
      linearUpdate: 0.1
      angularUpdate: 0.1
      temporalUpdate: 3.0
      resampleThreshold: 0.5
      particles: 80
      xmin: -5.0
      ymin: -5.0
      xmax: 5.0
      ymax: 5.0
      delta: 0.025
      llsamplerange: 0.01
      llsamplestep: 0.01
      lasamplerange: 0.005
      lasamplestep: 0.005
      base_frame: base_footprint
    </rosparam>
  </node> 

</launch>